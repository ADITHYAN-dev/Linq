{
    "builds": [
      {
        "src": "videoconf/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "Python 3.12.7"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)", 
        "dest": "videoconf/wsgi.py"
      }
    ]
  }